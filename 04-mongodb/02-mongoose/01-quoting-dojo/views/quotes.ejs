<html>
    <head>
        <title>Display Quotes</title>
        <link rel="stylesheet" href="quotes.css">
    </head>
    <body>
        <div id="container">
            <h1>Here are the awesome quotes!</h1>
            <ul>
                <% for(var i = quotes.length-1; i > -1; i--) { %>
                    <li>"<%= quotes[i]['quote']%>"</li>
                    <li class="quoteAuthor">-<%= quotes[i]['name']%> at <%= helper.formatDate(quotes[i]['createdAt']) %></li>
                <% } %>
            </ul>
        </div>
    </body>
</html>